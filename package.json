{
  "name": "mom-bot-transcript-service",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:frontend && npm run build:backend && npm run copy:frontend",
    "build:backend": "tsc",
    "build:frontend": "cd frontend && npm install && npm run build",
    "copy:frontend": "mkdir -p dist/public && cp -r frontend/dist/* dist/public/",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["teams", "transcript", "meeting", "automation"],
  "author": "Hackathon Team",
  "license": "ISC",
  "description": "Automated meeting transcript fetcher for Microsoft Teams",
  "dependencies": {
    "@azure/identity": "^4.13.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "axios": "^1.13.4",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-session": "^1.18.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.0",
    "@types/node": "^25.2.0",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
